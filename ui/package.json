{
  "name": "floppyui",
  "version": "1.0.0",
  "private": false,
  "publishConfig": {
    "registry": "https://nexus.area51.dev/repository/npm-private/"
  },
  "scripts": {
    "build": "npm run babel && npm run webpack",
    "babel": "babel src --ignore __tests__ --out-dir ./build",
    "webpack": "webpack --config webpack.config.js && cp -v public/* dist",
    "lint": "eslint ./src",
    "lintfix": "eslint ./src --fix",
    "lint:watch": "esw -w src/**",
    "cleanrun": "rm -rf dist build && npm run run",
    "run": "npm run build && npm run dev",
    "devhot": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --hot --inline --history-api-fallback",
    "dev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --history-api-fallback"
  },
